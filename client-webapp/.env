# Where the auth server is running (no trailing slash)
VITE_AUTH_URL=http://localhost:5001

# The client_id you’ve registered (must match what auth‐server expects)
VITE_CLIENT_ID=my-client-id

# The exact redirect URI you configured in auth‐server
VITE_REDIRECT_URI=http://localhost:3000/callback

# Where the resource server is running
VITE_RESOURCE_URL=http://localhost:5000
